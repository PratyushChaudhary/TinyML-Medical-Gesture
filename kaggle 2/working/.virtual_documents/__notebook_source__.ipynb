    'flash_kb': EDGE_IMPULSE_METRICS['flash_kb'] * 4,
    'latency_ms': EDGE_IMPULSE_METRICS['latency_ms'] * 1.3,  # Quantized is faster
    'accuracy': EDGE_IMPULSE_METRICS['accuracy'] + 0.5  # Minimal accuracy loss
}

# ============================================
# Create Comparison Table
# ============================================
def create_comparison_table():
    """
    Create the optimization comparison table
    """
    data = {
        'Metric': ['Flash Usage', 'RAM Usage', 'Latency', 'Accuracy'],
        'Float32 (Standard)': [
            f"{FLOAT32_ESTIMATES['flash_kb']:.1f} KB",
            f"{FLOAT32_ESTIMATES['ram_kb']:.1f} KB",
            f"{FLOAT32_ESTIMATES['latency_ms']:.1f} ms",
            f"{FLOAT32_ESTIMATES['accuracy']:.1f}%"
        ],
        'Int8 (Quantized)': [
            f"{EDGE_IMPULSE_METRICS['flash_kb']:.1f} KB",
            f"{EDGE_IMPULSE_METRICS['ram_kb']:.1f} KB",
            f"{EDGE_IMPULSE_METRICS['latency_ms']:.1f} ms",
            f"{EDGE_IMPULSE_METRICS['accuracy']:.1f}%"
        ],
        'Reduction': []
    }
    
    # Calculate reductions
    flash_reduction = ((FLOAT32_ESTIMATES['flash_kb'] - EDGE_IMPULSE_METRICS['flash_kb']) / 
                       FLOAT32_ESTIMATES['flash_kb']) * 100
    ram_reduction = ((FLOAT32_ESTIMATES['ram_kb'] - EDGE_IMPULSE_METRICS['ram_kb']) / 
                     FLOAT32_ESTIMATES['ram_kb']) * 100
    latency_reduction = ((FLOAT32_ESTIMATES['latency_ms'] - EDGE_IMPULSE_METRICS['latency_ms']) / 
                         FLOAT32_ESTIMATES['latency_ms']) * 100
    accuracy_loss = FLOAT32_ESTIMATES['accuracy'] - EDGE_IMPULSE_METRICS['accuracy']
    
    data['Reduction'] = [
        f"‚Üì {flash_reduction:.1f}%",
        f"‚Üì {ram_reduction:.1f}%",
        f"‚Üì {latency_reduction:.1f}%",
        f"‚Üì {accuracy_loss:.1f}% loss"
    ]
    
    df = pd.DataFrame(data)
    
    # Print table
    print("\n" + "=" * 80)
    print("TINYML OPTIMIZATION COMPARISON")
    print("=" * 80)
    print(df.to_string(index=False))
    print("=" * 80)
    
    # Save to CSV
    csv_path = os.path.join(OUTPUT_DIR, "optimization_comparison.csv")
    df.to_csv(csv_path, index=False)
    print(f"\nSaved to: {csv_path}")
    
    return df

# ============================================
# Visualization 1: Bar Chart Comparison
# ============================================
def plot_optimization_bars():
    """
    Create bar chart comparing metrics
    """
    fig, axes = plt.subplots(1, 3, figsize=(16, 5))
    fig.suptitle('TinyML Optimization: Float32 vs Int8 Quantization', 
                 fontsize=18, fontweight='bold', y=1.02)
    
    metrics = [
        ('Flash Usage', 'flash_kb', 'KB'),
        ('RAM Usage', 'ram_kb', 'KB'),
        ('Latency', 'latency_ms', 'ms')
    ]
    
    colors = ['#3498db', '#2ecc71']
    
    for ax, (title, key, unit) in zip(axes, metrics):
        float_val = FLOAT32_ESTIMATES[key]
        int8_val = EDGE_IMPULSE_METRICS[key]
        reduction = ((float_val - int8_val) / float_val) * 100
        
        bars = ax.bar(['Float32', 'Int8'], [float_val, int8_val], 
                      color=colors, edgecolor='black', linewidth=2, alpha=0.85)
        
        ax.set_ylabel(unit, fontsize=12, fontweight='bold')
        ax.set_title(title, fontsize=14, fontweight='bold')
        ax.grid(axis='y', alpha=0.3, linestyle='--')
        
        # Value labels
        for bar, val in zip(bars, [float_val, int8_val]):
            height = bar.get_height()
            ax.text(bar.get_x() + bar.get_width()/2., height,
                    f'{val:.1f}',
                    ha='center', va='bottom', fontsize=11, fontweight='bold')
        
        # Reduction label
        ax.text(0.5, max(float_val, int8_val) * 0.85, 
                f'‚Üì {reduction:.1f}%', 
                ha='center', fontsize=13, fontweight='bold', 
                color='green',
                bbox=dict(boxstyle='round', facecolor='lightgreen', alpha=0.7))
    
    plt.tight_layout()
    output_path = os.path.join(OUTPUT_DIR, "4_optimization_comparison.png")
    plt.savefig(output_path, dpi=300, bbox_inches='tight')
    print(f"Saved: {output_path}")
    plt.close()

# ============================================
# Visualization 2: Memory Footprint
# ============================================
def plot_memory_footprint():
    """
    Stacked bar chart showing total memory
    """
    fig, ax = plt.subplots(figsize=(10, 6))
    
    categories = ['Float32\nModel', 'Int8 Quantized\nModel\n(Edge Impulse)']
    
    flash = [FLOAT32_ESTIMATES['flash_kb'], EDGE_IMPULSE_METRICS['flash_kb']]
    ram = [FLOAT32_ESTIMATES['ram_kb'], EDGE_IMPULSE_METRICS['ram_kb']]
    
    x = np.arange(len(categories))
    width = 0.5
    
    p1 = ax.bar(x, flash, width, label='Flash (Model)', 
                color='#3498db', edgecolor='black', linewidth=2)
    p2 = ax.bar(x, ram, width, bottom=flash, label='RAM (Runtime)', 
                color='#e67e22', edgecolor='black', linewidth=2)
    
    ax.set_ylabel('Memory (KB)', fontsize=13, fontweight='bold')
    ax.set_title('Total Memory Footprint Comparison', fontsize=16, fontweight='bold', pad=20)
    ax.set_xticks(x)
    ax.set_xticklabels(categories, fontsize=12)
    ax.legend(fontsize=11, loc='upper right')
    ax.grid(axis='y', alpha=0.3, linestyle='--')
    
    # Total labels
    for i, (f, r) in enumerate(zip(flash, ram)):
        total = f + r
        ax.text(i, total + 2, f'{total:.1f} KB', 
                ha='center', va='bottom', fontsize=12, fontweight='bold')
    
    # MCU limit line
    ax.axhline(y=256, color='green', linestyle='--', linewidth=2, alpha=0.7)
    ax.text(1.5, 260, '‚Üê Arduino Nano 33 BLE (256 KB)', 
            fontsize=10, color='green', fontweight='bold')
    
    # Success annotation
    ax.text(1, EDGE_IMPULSE_METRICS['flash_kb'] + EDGE_IMPULSE_METRICS['ram_kb'] / 2,
            '‚úì Fits on\nTinyML MCU!',
            ha='center', fontsize=12, fontweight='bold', color='white',
            bbox=dict(boxstyle='round', facecolor='green', alpha=0.8))
    
    plt.tight_layout()
    output_path = os.path.join(OUTPUT_DIR, "5_memory_footprint.png")
    plt.savefig(output_path, dpi=300, bbox_inches='tight')
    print(f"Saved: {output_path}")
    plt.close()

