import os
import pandas as pd

BASE_DIR = "/kaggle/input/imu-glove/rosbag/data"   
SUB_DIRS = ["data", "data_clean", "label"]

def get_sample_csv(folder_path):
    """Return the first CSV file found in a folder"""
    for file in os.listdir(folder_path):
        if file.endswith(".csv"):
            return os.path.join(folder_path, file)
    return None

def inspect_csv(csv_path, n_rows=5):
    """Print structure and a small sample of the CSV"""
    df = pd.read_csv(csv_path)

    print(f"\nüìÑ File: {csv_path}")
    print(f"üîπ Shape: {df.shape}")
    print(f"üîπ Columns ({len(df.columns)}):")
    print(list(df.columns))

    print(f"\nüîπ Sample ({n_rows} rows):")
    print(df.head(n_rows))

def main():
    print("=== CSV STRUCTURE SAMPLE REPORT ===")

    for sub in SUB_DIRS:
        folder_path = os.path.join(BASE_DIR, sub)

        print(f"\n\nüìÅ Inspecting folder: {folder_path}")

        if not os.path.exists(folder_path):
            print("‚ùå Folder not found")
            continue

        csv_file = get_sample_csv(folder_path)

        if csv_file is None:
            print("‚ö†Ô∏è No CSV files found")
            continue

        inspect_csv(csv_file)

if __name__ == "__main__":
    main()



import os 
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
from pathlib import Path
import warnings
warnings.filterwarnings('ignore')

# CONFIGURATION
BASE_DIR = "/kaggle/input/imu-glove/rosbag/data"   
DATA_CLEAN_DIR = os.path.join(BASE_DIR, "data_clean")
LABEL_DIR = os.path.join(BASE_DIR, "label")
OUTPUT_DIR = "./processed_data"

os.makedirs(OUTPUT_DIR, exist_ok=True)

GESTURE_MAP = {
    0: "STATIC",
    1: "SLIDE_UP",
    2: "SLIDE_DOWN",
    3: "SLIDE_LEFT",
    4: "SLIDE_RIGHT",
    5: "RELEASE",
    6: "GRASP",
    10: "NONE"
}

# Features used: (18 total: 6 per IMU √ó 3 IMUs)
FEATURES_IMU0 = [
    'Imu0_linear_accleration_x', 'Imu0_linear_accleration_y', 'Imu0_linear_accleration_z',
    'Imu0_angular_velocity_x', 'Imu0_angular_velocity_y', 'Imu0_angular_velocity_z'
]

FEATURES_IMU1 = [
    'Imu1_linear_accleration_x', 'Imu1_linear_accleration_y', 'Imu1_linear_accleration_z',
    'Imu1_angular_velocity_x', 'Imu1_angular_velocity_y', 'Imu1_angular_velocity_z'
]

FEATURES_IMU2 = [
    'Imu2_linear_accleration_x', 'Imu2_linear_accleration_y', 'Imu2_linear_accleration_z',
    'Imu2_angular_velocity_x', 'Imu2_angular_velocity_y', 'Imu2_angular_velocity_z'
]

ALL_FEATURES = FEATURES_IMU0 + FEATURES_IMU1 + FEATURES_IMU2

# Load dataset
def load_dataset():
    """
    Load all CSV files from data_clean and match with labels
    """
    data_files = sorted([f for f in os.listdir(DATA_CLEAN_DIR) if f.endswith('.csv')])
    label_files = sorted([f for f in os.listdir(LABEL_DIR) if f.endswith('.csv')])
    
    print(f"Found {len(data_files)} data files and {len(label_files)} label files")
    
    all_samples = []
    label_counts = {gesture: 0 for gesture in GESTURE_MAP.values()}
    
    for data_file in data_files:
        # Extract base name (remove _data.csv suffix)
        base_name = data_file.replace('_data.csv', '')
        label_file = base_name + '_label.csv'
        
        # Check if corresponding label exists
        if label_file not in label_files:
            print(f"No label found for {data_file}, skipping...")
            continue
        
        # Load data and label
        data_path = os.path.join(DATA_CLEAN_DIR, data_file)
        label_path = os.path.join(LABEL_DIR, label_file)
        
        df_data = pd.read_csv(data_path)
        df_label = pd.read_csv(label_path)
        
        label_num = df_label['label'].iloc[0]
        gesture_name = GESTURE_MAP.get(label_num, f"UNKNOWN_{label_num}")
        
        # Extract features
        try:
            features = df_data[ALL_FEATURES].values  # Shape: (100, 18)
            
            sample = {
                'filename': data_file,
                'label_num': label_num,
                'gesture': gesture_name,
                'features': features,
                'num_samples': len(features)
            }
            
            all_samples.append(sample)
            label_counts[gesture_name] += 1
            
        except KeyError as e:
            print(f"Missing features in {data_file}: {e}")
            continue
    
    print(f"\nSuccessfully loaded {len(all_samples)} samples")
    print("\nClass Distribution:")
    for gesture, count in sorted(label_counts.items()):
        print(f"   {gesture:15s}: {count:3d} samples")
    
    return all_samples, label_counts

# Function: Create Visualization
def create_viz_dataset(all_samples):
    """
    Create a flat DataFrame 
    """
    rows = []
    
    for sample in all_samples:
        features = sample['features']  # (100, 18)
        gesture = sample['gesture']
        
        for i in range(len(features)):
            row = {
                'gesture': gesture,
                'sample_idx': i,
                'filename': sample['filename']
            }
            
            # Add IMU0 features
            row['Ax_0'] = features[i, 0]
            row['Ay_0'] = features[i, 1]
            row['Az_0'] = features[i, 2]
            row['Gx_0'] = features[i, 3]
            row['Gy_0'] = features[i, 4]
            row['Gz_0'] = features[i, 5]
            
            # Add IMU1 features
            row['Ax_1'] = features[i, 6]
            row['Ay_1'] = features[i, 7]
            row['Az_1'] = features[i, 8]
            row['Gx_1'] = features[i, 9]
            row['Gy_1'] = features[i, 10]
            row['Gz_1'] = features[i, 11]
            
            # Add IMU2 features
            row['Ax_2'] = features[i, 12]
            row['Ay_2'] = features[i, 13]
            row['Az_2'] = features[i, 14]
            row['Gx_2'] = features[i, 15]
            row['Gy_2'] = features[i, 16]
            row['Gz_2'] = features[i, 17]
            
            rows.append(row)
    
    df = pd.DataFrame(rows)
    return df

if __name__ == "__main__":
    print("=" * 60)
    print("TinyML Gesture Recognition - Data Preparation")
    print("=" * 60)
    
    # Load dataset
    all_samples, label_counts = load_dataset()
    
    # Create visualization dataset
    print("\nCreating visualization-ready dataset...")
    df_viz = create_viz_dataset(all_samples)
    
    print(f"Created dataset with shape: {df_viz.shape}")
    print(f"   Columns: {list(df_viz.columns)}")
    
    # Save to CSV
    output_path = os.path.join(OUTPUT_DIR, "complete_dataset.csv")
    df_viz.to_csv(output_path, index=False)
    print(f"\nSaved complete dataset to: {output_path}")
    
    # Save metadata
    metadata = {
        'total_samples': len(all_samples),
        'total_timesteps': len(df_viz),
        'num_features': 18,
        'num_classes': len([c for c in label_counts.values() if c > 0]),
        'class_distribution': label_counts
    }
    
    print("\n" + "=" * 60)
    print("DATASET SUMMARY")
    print("=" * 60)
    print(f"Total Gesture Recordings: {metadata['total_samples']}")
    print(f"Total Timesteps: {metadata['total_timesteps']}")
    print(f"Features per Timestep: {metadata['num_features']}")
    print(f"Number of Classes: {metadata['num_classes']}")
    print("\nData preparation complete!")


import pandas as pd
