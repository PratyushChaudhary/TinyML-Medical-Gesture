import pandas as pd
import numpy as np
import os
from sklearn.model_selection import train_test_split

# ============================================
# CONFIGURATION
# ============================================
DATA_FILE = "./processed_data/complete_dataset.csv"
OUTPUT_DIR = "./edge_impulse_data"
TRAIN_DIR = os.path.join(OUTPUT_DIR, "training")
TEST_DIR = os.path.join(OUTPUT_DIR, "testing")

os.makedirs(TRAIN_DIR, exist_ok=True)
os.makedirs(TEST_DIR, exist_ok=True)

# ============================================
# Create Individual CSV Files (BEST METHOD)
# ============================================
def create_individual_csv_files():
    """
    Create one CSV file per gesture sample
    This is the most reliable way for Edge Impulse
    """
    df = pd.read_csv(DATA_FILE)
    
    # Group by filename
    grouped = df.groupby('filename')
    
    all_files = []
    
    for filename, group in grouped:
        gesture = group['gesture'].iloc[0]
        
        # Create a clean dataframe for this sample
        sample_df = pd.DataFrame({
            'timestamp': range(0, len(group) * 20, 20),  # 0, 20, 40, ..., 1980 ms
            'ax0': group['Ax_0'].values,
            'ay0': group['Ay_0'].values,
            'az0': group['Az_0'].values,
            'gx0': group['Gx_0'].values,
            'gy0': group['Gy_0'].values,
            'gz0': group['Gz_0'].values,
            'ax1': group['Ax_1'].values,
            'ay1': group['Ay_1'].values,
            'az1': group['Az_1'].values,
            'gx1': group['Gx_1'].values,
            'gy1': group['Gy_1'].values,
            'gz1': group['Gz_1'].values,
            'ax2': group['Ax_2'].values,
            'ay2': group['Ay_2'].values,
            'az2': group['Az_2'].values,
            'gx2': group['Gx_2'].values,
            'gy2': group['Gy_2'].values,
            'gz2': group['Gz_2'].values
        })
        
        all_files.append({
            'gesture': gesture,
            'filename': filename,
            'data': sample_df
        })
    
    # Create labels array for stratified split
    labels = [f['gesture'] for f in all_files]
    
    # Split 80/20
    train_files, test_files = train_test_split(
        all_files,
        test_size=0.2,
        random_state=42,
        stratify=labels
    )
    
    print(f"Total samples: {len(all_files)}")
    print(f"   Training: {len(train_files)}")
    print(f"   Testing: {len(test_files)}")
    
    # Save training files
    train_counts = {}
    for i, file_data in enumerate(train_files):
        gesture = file_data['gesture']
        train_counts[gesture] = train_counts.get(gesture, 0) + 1
        
        safe_filename = f"{gesture}_{train_counts[gesture]}.csv"
        output_path = os.path.join(TRAIN_DIR, safe_filename)
        file_data['data'].to_csv(output_path, index=False)
    
    # Save testing files
    test_counts = {}
    for i, file_data in enumerate(test_files):
        gesture = file_data['gesture']
        test_counts[gesture] = test_counts.get(gesture, 0) + 1
        
        safe_filename = f"{gesture}_{test_counts[gesture]}.csv"
        output_path = os.path.join(TEST_DIR, safe_filename)
        file_data['data'].to_csv(output_path, index=False)
    
    print(f"\nTraining set distribution:")
    for gesture in sorted(train_counts.keys()):
        print(f"   {gesture:15s}: {train_counts[gesture]:3d} samples")
    
    print(f"\nTesting set distribution:")
    for gesture in sorted(test_counts.keys()):
        print(f"   {gesture:15s}: {test_counts[gesture]:3d} samples")
    
    print(f"\nFiles saved to:")
    print(f"   Training: {TRAIN_DIR}")
    print(f"   Testing: {TEST_DIR}")
    
    return train_files, test_files

# ============================================
# MAIN
# ============================================
if __name__ == "__main__":
    print("=" * 60)
    print("Creating Individual CSV Files for Edge Impulse")
    print("=" * 60)
    
    train, test = create_individual_csv_files()
    
    print("\n" + "=" * 60)
    print("File creation complete!")
    print("=" * 60)
    print("\nUPLOAD INSTRUCTIONS:")
    print("1. Go to: https://studio.edgeimpulse.com/")
    print("2. Create project ‚Üí Time series data")
    print("3. Data acquisition ‚Üí Upload data")
    print("4. Upload ALL files from 'training' folder ‚Üí Category: Training")
    print("5. Upload ALL files from 'testing' folder ‚Üí Category: Testing")
    print("6. When asked about labels:")
    print("   - Choose 'Infer from filename'")
    print("   - Pattern: [LABEL]_[number].csv")
    print("7. For time series data:")
    print("   - Check 'Data has timestamps'")
    print("   - Timestamp column: 'timestamp'")
    print("=" * 60)


